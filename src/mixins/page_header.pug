mixin page_header(active)
  header 
    a.skip-to-content-link(href="#main") Skip to content
    a.logo(href="/")
      //- img(src=require('Images/walkers-creek-logo.png'))
      h1 Company Logo
    nav  
      ul 
        - var pages = {'home': '/', 'about': '/about'}
        each val, key in pages
          li
            a(href= val)(class= active.toLowerCase() === key ? 'current': undefined) #{key}
        