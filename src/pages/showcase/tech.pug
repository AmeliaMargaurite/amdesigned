extends ../../mixins/page_layout.pug
include ../../components/image-w-placeholder/image-w-placeholder
include ../../components/images/image-size-fraction

block variables
	- var title = "Examples";
	- var page_description = "";
	- var slug = "/";
	//- - var page_image = require('../images/web-sites-small-medium-business-ecommerce-seo.jpg')
	- var page_image_alt = "";

block variables
	- var title = "Tech";
	- var pageClass = "tech basics";

block scss-files
	link(href=require("../../tech-page/main.scss"), rel="stylesheet")

block content
	.header-cta__wrapper.dark
		header
			p.logo Morpheus AI

		section.cta
			.section__content.split-view
				.split-view__left
					.wrapper
						p.subtext.top Top tagline to sound cool
						h1 Leaders in robotic AI tech
						p.subtext.bottom Challenging the Morpheus concept, one AI at a time
					button.cta.btn Discover more
						span.icon__wrapper
							span.icon.arrow.right
				.split-view__right
					+image-size-fraction({path: "robot-girl.png", alt: 'rendered image of shiny female robot face', fractions: [1, 1, 1, 1, 0.5, 0.5, 0.25]})

	section.services__section.light-main
		.section__content
			p.section__title Services
			h2.subtext We provide the following amazing services
			.cards
				article.card
					span.icon--wrapper
						span.icon.laptop-code.large
					p.card__title Machine Learning
					p.card__content.paragraph Lorem ipsum dolor sit amet consectetur adipisicing elit. Enim perspiciatis quidem itaque iste reprehenderit.
				article.card
					span.icon--wrapper
						span.icon.glasses.large
					p.card__title Augmented Reality
					p.card__content.paragraph Lorem ipsum dolor sit amet consectetur adipisicing elit. Enim perspiciatis quidem itaque iste reprehenderit.
				article.card
					span.icon--wrapper
						span.icon.industry.large
					p.card__title Automation
					p.card__content.paragraph Lorem ipsum dolor sit amet consectetur adipisicing elit. Enim perspiciatis quidem itaque iste reprehenderit.

	section.about__section
		.section__content.split-view
			.split-view__left
				.wrapper
					span.img.img-1
						+image-size-fraction({path: "tech-laptop.jpg", alt:"hands on a laptop keyboard", fractions: [0, 0, 0, 0.1, 0.1, 0.1, 0.1]})
					span.img.img-2
						+image-size-fraction({path: "tech-microscope.jpg", alt:"microscope viewing unknown green item", fractions: [0.75, 0.75, 0.75, 0.75, 0.25, 0.25, 0.25]})
					span.img.img-3
						+image-size-fraction({path: "tech-v-reality.jpg", alt:"person wearing virtual reality goggles", fractions: [0, 0, 0, 0.2, 0.2, 0.2, 0.2]})
			.split-view__right.gap
				.wrapper
					p.section__title.left-align About
					h2.subtext We grew from a seed, and now look where we are
					p.about__content.paragraph Lorem, ipsum dolor sit amet consectetur adipisicing elit. Reiciendis tempore omnis aut incidunt eum autem rem, facilis quasi vero fugiat? Lorem ipsum dolor sit amet consectetur adipisicing elit. Totam, suscipit?
