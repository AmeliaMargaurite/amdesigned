						li Lorem ipsum dolor sit.
						li Lorem ipsum dolor sit amet consectetur.
						li Lorem ipsum dolor sit amet.
				button.btn.secondary.cta Discover more
					span.icon__wrapper
						span.icon.arrow.right

	section.projects__section.dark
		.section__content.split-view
			.split-view__left.gap
				.wrapper
					p.section__title.left-align Projects
