								span.icon.robot
							.content
								p.stamp__number 135+
								p.stamp__title Finalised projects

						article.stamp
							span.stamp__icon--wrapper
								span.icon.user-astronaut
							.content
								p.stamp__number 90+
								p.stamp__title Happy customers

			.split-view__right
				.wrapper
					+image-size-fraction({path: "tech-shopping-cart.png", alt: 'test', fractions: [0.75, 0.75, 0.75, 0.75, 0.4, 0.4, 0.4]})

	section.testimonials__section
		.section__content
			p.section__title Testimonials
			h2.subtext What our clients have to say

			.quotes__wrapper
				article.quote
					span.icon--wrapper
						span.icon.quote-left.large
					span.quote__content
